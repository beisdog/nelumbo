<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="Products"
		class="sapUiPopupWithPadding"
		items="{formConfig>/values}"
		search="_handleValueHelpSearch"
		confirm="_handleValueHelpClose"
		cancel="_handleValueHelpClose">
		<StandardListItem
			title="{key}"
			description="{key}" />
	</SelectDialog>
</core:FragmentDefinition>